## User
VM_USER=$(logname)

## IOMMU groups
IOMMU_GPU=
IOMMU_GPU_AUDIO=
IOMMU_PCI_AUDIO=

## For extracting the VBIOS
VBIOS_EXTRACT_PATH=/root/VBIOS.rom
NVFLASH=/root/nvflash_linux
extract_videobusid="0000:$IOMMU_GPU"

## Virsh devices
VIRSH_GPU=
VIRSH_GPU_AUDIO=
VIRSH_PCI_AUDIO=

## PCI BUS ID
videoid=
audioid=
videobusid="0000:$IOMMU_GPU"
audiobusid="0000:$IOMMU_GPU_AUDIO"
pciaudioid=
pciaudiobusid="0000:$IOMMU_PCI_AUDIO"

## QEMU options
RAM=
CORES=

## Hugepages
HUGEPAGES=

## Display manager
DSPMGR=

## Input devices
EVENTIF01=
EVENTKBD=
EVENTMOUSE=

## USB DEVICE(s) ID(s)
usb1_vendorid=0x
usb1_productid=0x

## Log file
LOG=

## Local path for storage
IMAGES=

## VBIOS
VBIOS=$IMAGES/VBIOS.rom

## Windows
VIRTIO=$IMAGES/iso/virtio-win.iso
OVMF=/usr/share/OVMF/x64/OVMF_CODE.fd
OVMF_VARS=/usr/share/OVMF/x64/OVMF_VARS.fd

## MacOS
MACOS_ESP=$IMAGES/macos/ESP.qcow2
MACOS_OVMF_CODE=$IMAGES/macos/firmware/OVMF_CODE.fd
MACOS_OVMF_VARS=$IMAGES/macos/firmware/OVMF_VARS-1024x768.fd

## Ulimit
ULIMIT=$(ulimit -a | grep "max locked memory" | awk '{print $6}')
ULIMIT_TARGET=$(( $(echo $RAM | tr -d 'G')*1048576+100000 ))

